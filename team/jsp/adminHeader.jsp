<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html>
<html>

<head>

<meta charset="UTF-8">

<link rel="stylesheet" href="./css/admin.css">

<title>ヘッダー</title>

</head>

<body>
	<header>

		<div id= "header" >


			<!-- ヘッダーロゴ -->
  			<div id="header-rogo">
 			<!-- <a href="home.jsp"> -->
 					<img src="images/logo.jpg"/>


			</div>

			<div id="header-menu">
				<ul>

						<!-- ログアウト -->
						<s:if test="#session.logined==1">
							<li id="icon"><a href=<s:url action="LogoutAction"/>>
							<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_logout.jpg" id="active"/>
 								</a>
 								</li>
						</s:if>

						<!-- ログイン -->
						<s:else>
							<li id="icon"><a href="GoLoginAction">
							<img src="data:image/svg+xml;utf8;base64," />
 								<img src="./images/icon_login.jpg" id="active"/>
 								</a>
 								</li>
						</s:else>

						<!-- 管理者ページ -->
						<s:if test="#session.status == 1">
							<li id="icon"><a href=<s:url action="AdminAction"/>>
								<img src="data:image/svg+xml;utf8;base64," />
								<img src="./images/icon_admin.jpg" id="active"/>
								</a>
 								</li>
						</s:if>

						<li></li>
						<li></li>

				</ul>
			</div>
		</div>
		<div class="clearfix">
		</div>
	</header>
</body>
</html>